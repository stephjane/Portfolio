@import "bootstrap-sprockets";
@import "bootstrap";

.container {
  background-color: #24464A;
  
  width: 100%;
  height: 100%;
  padding-left: 0px;
  padding-right: 0px;
  margin-right: 0px;
  margin-left: 0px;
}

.top {
  top: 0;
  left: 0;
  background-color: #FFFFFF;
  height: auto;
  width: 100%;
  padding-bottom: 10px;
  border-bottom: 2px solid #000000;
}
.bar{

}

.background {
}
.resume-button {
  margin-top: 5px;
  text-align: center;
}

#name_logo {
  margin-top: 10px;
  margin-bottom: 10px;
  img {
    display: block;
    max-width: 100%;
    max-height: 100%;
  }
}

.name_panel{
  bottom: 0;
  left: 0;
  background-color: #FFFFFF;
  height: 100%;
  width: 100%;
  border-top: 2px solid #000000;
  padding-top: 15px;
}

#hero {
  padding-top: 10px;
  padding-bottom: 10px;
}

@media screen and (min-width: 900px) {
  .top{
    top:0;
    left: 0;
    background-color: #FFFFFF;
    height: auto;
    padding-bottom: 10px;
    border-bottom: 2px solid #000000;

  }
  .bar{
    width:900px;
  }

